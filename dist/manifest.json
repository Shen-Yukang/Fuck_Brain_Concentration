{
  "manifest_version": 3,
  "default_locale": "en",
  "name": "__MSG_extensionName__",
  "browser_specific_settings": {
    "gecko": {
      "id": "example@example.com",
      "strict_min_version": "109.0"
    }
  },
  "version": "1.2.0",
  "description": "__MSG_extensionDescription__",
  "host_permissions": [
    "<all_urls>"
  ],
  "permissions": [
    "storage",
    "scripting",
    "tabs",
    "notifications",
    "offscreen"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_title": "Fuck_Brain_Concentration",
    "default_popup": "popup/index.html",
    "default_icon": {
      "16": "spring-16.png",
      "48": "spring-48.png",
      "128": "spring-128.png"
    }
  },
  "icons": {
    "128": "spring-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "<all_urls>"
      ],
      "js": [
        "content/index.iife.js"
      ]
    },
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "<all_urls>"
      ],
      "css": [
        "content.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "spring-16.png",
        "spring-48.png",
        "spring-128.png",
        "notification.mp3",
        "offscreen.html",
        "offscreen.js",
        "blocked.html",
        "blocked.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}